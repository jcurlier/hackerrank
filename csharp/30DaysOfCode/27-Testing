using System;
using System.Collections.Generic;
using System.IO;
class Solution 
{
    static void Main(String[] args) 
    {
        Console.WriteLine("5");
        Console.WriteLine("1 1");
        Console.WriteLine("-1");
        Console.WriteLine("1 1");
        Console.WriteLine("2");
        Console.WriteLine("1 1");
        Console.WriteLine("1");
        Console.WriteLine("3 2");
        Console.WriteLine("-1 2 3");
        Console.WriteLine("3 2");
        Console.WriteLine("-1 3 -2");
    }
}